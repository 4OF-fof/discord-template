{
  "name": "bot-template",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "register": "tsx src/register.ts",
    "dev": "tsx src/index.ts",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.5.1",
    "oxfmt": "^0.16.0",
    "oxlint": "^0.16.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "discord.js": "^14.17.0",
    "dotenv": "^17.2.3"
  }
}
